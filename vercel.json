{
    "build": {
        "env": {
            "NODE_VERSION": "20"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/reply.js": {
            "runtime": "@vercel/node@3.0.0",
            "maxDuration": 30
        }
    }
}